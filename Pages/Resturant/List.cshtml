@page
@model OdetoCodeDemoWebApp.Pages.Resturant.ListModel
@{
}
<h1>Resturants</h1>

<table class="table">

    @foreach(var resturant in Model.resturants){
    <tr>
        <td>@resturant.Name</td>
        <td>@resturant.Location</td>
        <td>@resturant.CusineType</td>
    </tr>
    }
</table>

<div>@Model.Message</div>